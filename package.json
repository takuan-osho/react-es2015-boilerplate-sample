{
  "name": "react-es2015-boilerplate-sample",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "build": "browserify -t babelify src/js/app.js -o dist/bundle.js",
    "watch:server": "browser-sync start --server . --files 'dist/*.*,*.html'",
    "watch:js": "watchify -t babelify src/js/app.js -o dist/bundle.js",
    "watch": "npm-run-all -p watch:js watch:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react"
  ],
  "author": "SHIMIZU Taku",
  "license": "MIT",
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browser-sync": "^2.10.0",
    "eslint": "^1.10.2",
    "eslint-config-google": "^0.3.0",
    "npm-run-all": "^1.3.2",
    "watchify": "^3.6.1"
  }
}
